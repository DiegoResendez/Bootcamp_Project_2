<ul>
  {{#each items}}
  	<li>
  	   <p>ID: {{this.id}}</p>
       <p>Item: {{this.item}}</p>
  	</li>
  {{/each}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="item"></textarea>
	<button type="submit">Add Item</button>
</form>

<form action="/" method="UPDATE">
	<textarea type="text" name="item"></textarea>
<button type="update">Edit Item</button>
</form>

<form action="/" method="DELETE">
	<textarea type="text" name="item"></textarea>
<button type="delete">Delete Item</button>
</form>

<form action="/" method="SEARCH">
	<textarea type="text" name="item"></textarea>
<button type="search">Search Amazon</button>
</form>